# R build ignore patterns for newrllama4 package

# Git and version control
^\.git.*
^\.gitignore$
^\.gitattributes$

# GitHub
^\.github.*
^README\.Rmd$
^README\.md$

# Project files
^.*\.Rproj$
^\.Rproj\.user$

# Development and testing
^\.Rhistory$
^\.RData$
^\.Ruserdata$
^\.DS_Store$
^Thumbs\.db$

# Build artifacts  
^.*\.o$
^.*\.so$
^.*\.dll$
^.*\.dylib$
^.*\.tar\.gz$

# Documentation
^doc$
^docs$
^Meta$

# CI/CD
^\.travis\.yml$
^appveyor\.yml$
^codecov\.yml$

# Cache and temporary files
^\.cache$
^cache$
^tmp$
^temp$

# IDE files
^\.vscode$
^\.idea$

# Custom project files (outside of R package structure)
^backend/
^custom_files/
^高层次函数开发方案\.md$

# Package management
^renv$
^renv\.lock$
^packrat$

# Large files that shouldn't be in package
^models/
^data-raw/.*\.gguf$
^inst/extdata/.*\.gguf$

# Test coverage
^codecov\.yml$
^\.codecov\.yml$

# pkgdown site
^_pkgdown\.yml$
^pkgdown$

# Misc
^CRAN-RELEASE$
^CRAN-SUBMISSION$
^cran-comments\.md$